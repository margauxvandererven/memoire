Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {stars: abundances, stars: AGB and post-AGB, stars: atmospheres, stars: individual: WD 0044–121, stars: individual: WD 1751+106, stars: individual: WD 2134+25},
	pages = {65},
	file = {Full Text PDF:/Users/margauxvandererven/Zotero/storage/QIB9EBDI/Löbling - 2018 - Sliding along the Eddington Limit—Heavy-Weight Cen.pdf:application/pdf},
}



