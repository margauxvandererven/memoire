@article{kappeler_s-process_1989,
	title = {s-process nucleosynthesis-nuclear physics and the classical model},
	volume = {52},
	issn = {0034-4885},
	url = {https://dx.doi.org/10.1088/0034-4885/52/8/002},
	doi = {10.1088/0034-4885/52/8/002},
	abstract = {Among the various processes responsible for the formation of the heavy elements in stars, the slow neutron capture process (s-process) is distinguished by the fact that it involves mostly stable isotopes. Therefore, the relevant nuclear physics data can be determined by experiments. With this rather reliable data basis, s-process nucleosynthesis offers an important testground of models for the late stages of stellar evolution, which are supposed to be the s-process site. The empirical counterpart for such models is the so-called classical s-process, a purely phenomenological picture, that is successfully used to derive the resulting abundances as well as information on the physical conditions during the s-process. The status of this classical approach is reviewed with emphasis on the implications for various stellar models of the s-process and in the light of results obtained by stellar spectroscopy. A brief account of the potential s-process chronometers is also presented.},
	language = {en},
	number = {8},
	urldate = {2024-04-11},
	journal = {Reports on Progress in Physics},
	author = {Kappeler, F. and Beer, H. and Wisshak, K.},
	month = aug,
	year = {1989},
	pages = {945},
}

@article{nandakumar_m_2023-1,
	title = {M giants with {IGRINS} - {I}. {Stellar} parameters and α-abundance trends of the solar neighborhood population},
	volume = {675},
	copyright = {© The Authors 2023},
	issn = {0004-6361, 1432-0746},
	url = {https://www.aanda.org/articles/aa/abs/2023/07/aa46149-23/aa46149-23.html},
	doi = {10.1051/0004-6361/202346149},
	abstract = {{\textless}i{\textgreater}Context.{\textless}i/{\textgreater} Cool stars, such as M giants, can only be analyzed in the near-infrared (NIR) regime due to the ubiquitous titanium oxide features in optical spectra of stars with {\textless}i{\textgreater}T{\textless}i/{\textgreater}{\textless}sub{\textgreater}eff{\textless}sub/{\textgreater} {\textless} 4000 K. In dust-obscured regions, the inner bulge and Galactic center region, the intrinsically bright M giants observed in the NIR are an optimal option for studying stellar abundances and the chemical evolution of stellar populations. Because of the uncertainties in photometric methods, a method for determining the stellar parameters for M giants from the NIR spectra themselves is needed.{\textless}i{\textgreater}Aims.{\textless}i/{\textgreater} We develop a method for determining the stellar parameters for M giants from the NIR spectra. We validate the method by deriving the stellar parameters for nearby well-studied M giants with spectra from the spectral library of the Immersion GRating INfrared Spectrograph (IGRINS). We demonstrate the accuracy and precision of our method by determining the stellar parameters and {\textless}i{\textgreater}α{\textless}i/{\textgreater}-element trends versus metallicity for solar neighborhood M giants.{\textless}i{\textgreater}Methods.{\textless}i/{\textgreater} We carried out new observations of 44 M giant stars with IGRINS mounted on the Gemini South telescope. We also obtained the full {\textless}i{\textgreater}H{\textless}i/{\textgreater} and {\textless}i{\textgreater}K{\textless}i/{\textgreater} band IGRINS spectra of six nearby well-studied M giants at a spectral resolving power of {\textless}i{\textgreater}R{\textless}i/{\textgreater} = 45 000 from the IGRINS spectral library. We used the tool called spectroscopy made easy in combination with one-dimensional (1D) model atmospheres in a radiative and convective scheme (MARCS) stellar atmosphere models to model the synthetic spectrum that fits the observed spectrum best.{\textless}i{\textgreater}Results.{\textless}i/{\textgreater} The effective temperatures that we derive from our new method (tested for 3400 ≲ {\textless}i{\textgreater}T{\textless}i/{\textgreater}{\textless}sub{\textgreater}eff{\textless}sub/{\textgreater} ≲ 4000 K here) agree excellently with those of the six nearby well-studied M giants, which indicates that the accuracy is indeed high. For the 43 solar neighborhood M giants, our {\textless}i{\textgreater}T{\textless}i/{\textgreater}{\textless}sub{\textgreater}eff{\textless}sub/{\textgreater}, log {\textless}i{\textgreater}g{\textless}i/{\textgreater}, [Fe/H], {\textless}i{\textgreater}ξ{\textless}i/{\textgreater}{\textless}sub{\textgreater}micro{\textless}sub/{\textgreater}, [C/Fe], [N/Fe], and [O/Fe] agree with APOGEE with mean differences and a scatter (our method – APOGEE) of −67±33 K, −0.31±0.15 dex, 0.02±0.05 dex, 0.22±0.13 km s{\textless}sup{\textgreater}−1{\textless}sup/{\textgreater}, −0.05±0.06 dex, 0.06±0.06 dex, and 0.02±0.09 dex, respectively. Furthermore, the tight offset with a small dispersion compared to the APOGEE {\textless}i{\textgreater}T{\textless}i/{\textgreater}{\textless}sub{\textgreater}eff{\textless}sub/{\textgreater} indicates a high precision in our derived temperatures and those derived from the APOGEE pipeline. The typical uncertainties in the stellar parameters are found to be ±100 K in {\textless}i{\textgreater}T{\textless}i/{\textgreater}{\textless}sub{\textgreater}eff{\textless}sub/{\textgreater}, ±0.2 dex in log {\textless}i{\textgreater}g{\textless}i/{\textgreater}, ±0.1 dex in [Fe/H], and ±0.1 km s{\textless}sup{\textgreater}−1{\textless}sup/{\textgreater} in {\textless}i{\textgreater}ξ{\textless}i/{\textgreater}{\textless}sub{\textgreater}micro{\textless}sub/{\textgreater}. The {\textless}i{\textgreater}α{\textless}i/{\textgreater}-element trends versus metallicity for Mg, Si, Ca, and Ti are consistent with the APOGEE DR17 trends for the same stars and with the GILD optical trends. We also find a clear enhancement in the abundances for thick-disk stars.},
	language = {en},
	urldate = {2024-04-11},
	journal = {Astronomy \& Astrophysics},
	author = {Nandakumar, G. and Ryde, N. and Casagrande, L. and Mace, G.},
	month = jul,
	year = {2023},
	note = {Publisher: EDP Sciences},
	pages = {A23},
	file = {Full Text PDF:/Users/margauxvandererven/Zotero/storage/JIP9PRFL/Nandakumar et al. - 2023 - M giants with IGRINS - I. Stellar parameters and α.pdf:application/pdf},
}

