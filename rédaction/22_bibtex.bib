@article{shetye_s_2021,
	title = {S stars and s-process in the {Gaia} era - {II}. {Constraining} the luminosity of the third dredge-up with {Tc}-rich {S} stars},
	volume = {650},
	copyright = {© ESO 2021},
	issn = {0004-6361, 1432-0746},
	url = {https://www.aanda.org/articles/aa/abs/2021/06/aa40207-20/aa40207-20.html},
	doi = {10.1051/0004-6361/202040207},
	abstract = {{\textless}i{\textgreater}Context.{\textless}i/{\textgreater} S stars are late-type giants that are transition objects between M-type stars and carbon stars on the asymptotic giant branch (AGB). They are classified into two types: intrinsic or extrinsic, based on the presence or absence of technetium (Tc). The Tc-rich or intrinsic S stars are thermally pulsing (TP-)AGB stars internally producing {\textless}i{\textgreater}s{\textless}i/{\textgreater}-process elements (including Tc) that are brought to their surface via the third dredge-up (TDU). The Tc-poor or extrinsic S stars gained their {\textless}i{\textgreater}s{\textless}i/{\textgreater}-process overabundances via the accretion of {\textless}i{\textgreater}s{\textless}i/{\textgreater}-process-rich material from an AGB companion that has since turned into a dim white dwarf.{\textless}i{\textgreater}Aims.{\textless}i/{\textgreater} Our goal is to investigate the evolutionary status of Tc-rich S stars by locating them in a Hertzsprung-Russell (HR) diagram using the results of {\textless}i{\textgreater}Gaia{\textless}i/{\textgreater} Early Data Release 3 (EDR3). We combine the current sample of 13 Tc-rich stars with our previous studies of 10 Tc-rich stars to determine the observational onset of the TDU in the metallicity range [−0.7; 0]. We also compare our abundance determinations with dedicated AGB nucleosynthesis predictions.{\textless}i{\textgreater}Methods.{\textless}i/{\textgreater} We derived the stellar parameters using an iterative tool that combines HERMES high-resolution spectra, accurate {\textless}i{\textgreater}Gaia{\textless}i/{\textgreater} EDR3 parallaxes, stellar evolution models, and tailored MARCS model atmospheres for S-type stars. Using these stellar parameters, we determined the heavy-element abundances by line synthesis.{\textless}i{\textgreater}Results.{\textless}i/{\textgreater} In the HR diagram, the intrinsic S stars are located at higher luminosities than the predicted onset of the TDU. These findings are consistent with Tc-rich S stars being genuine TP-AGB stars. The comparison of the derived {\textless}i{\textgreater}s{\textless}i/{\textgreater}-process abundance profiles of our intrinsic S stars with the nucleosynthesis predictions provide an overall good agreement. Stars with highest [s/Fe] tend to have the highest C/O ratios.},
	language = {en},
	urldate = {2024-04-12},
	journal = {Astronomy \& Astrophysics},
	author = {Shetye, Shreeya and Eck, Sophie Van and Jorissen, Alain and Goriely, Stephane and Siess, Lionel and Winckel, Hans Van and Plez, Bertrand and Godefroid, Michel and Wallerstein, George},
	month = jun,
	year = {2021},
	note = {Publisher: EDP Sciences},
	pages = {A118},
	file = {Full Text PDF:/Users/margauxvandererven/Zotero/storage/P5R4IJY4/Shetye et al. - 2021 - S stars and s-process in the Gaia era - II. Constr.pdf:application/pdf},
}


